<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.romain.easyfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListeCommandeClient"
            android:label="EasyFood > Mes Commandes" />
        <activity
            android:name=".DetailsCommande"
            android:label="Mes Commandes > Ma Commande" />
        <activity
            android:name=".EspaceConnecte"
            android:label="EasyFood > Espace Connecté" />
        <activity
            android:name=".EspaceModerateur"
            android:label="Espace Modérateur" />
        <activity
            android:name=".DetailsPlat"
            android:label="Ma Commande > Mon Plat" />
        <activity
            android:name=".AjouterCommentairePlat" />
        <activity
            android:name=".DetailsResto"
            android:label="Details Resto">
        </activity>
    </application>

</manifest>